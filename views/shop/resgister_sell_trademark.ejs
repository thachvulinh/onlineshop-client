
<section class="banner_area">
    <div class="container">
        <div class="banner_text">
            <h3>Đăng ký bán hàng có thương hiệu</h3>
        </div>
    </div>
</section>
<section class="main_blog_area p_100">
    <div class="container">
        <div class="row">
            <div class="col-lg-4">
              <img src="../../img/blog/column/blog-c-1.jpg" style="width:100%;">
           </div>
            <div class="col-md-8">
                <div class="card">
                    <div class="card-body">
                        <a href="<%=url_admin%>/login" class="text-muted" style="font-size: 14px;"><i class="fa fa-arrow-circle-left"></i> Quay lại đăng nhập</a>
                        <%if(typeof error !=="undefined" &&  error!=""){%>
                        <div class="alert alert-danger" style="font-size:10px" role="alert">
                          <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                          </button>
                          <%-error%>
                        </div>
                        <%}%>
                        <form action="/users/resgister" method="Post" >
                          <input name="rank_id" type="hidden" value="6243525968810c11a154b677" />
                          <div class="row">
                              <div class="col-md-4">
                                <div class="form-group">
                                  <label for="name">Tên cửa hàng</label>
                                  <textarea class="form-control" name="name" id="name" rows="5"><%=(typeof data !== 'undefined'?data.name:'')%></textarea>
                                </div>
                              </div>
                              <div class="col-md-4">
                                <div class="form-group">
                                  <label for="phone">Số điện thoại</label>
                                  <input type="text" class="form-control" name="phone" id="phone" value="<%=(typeof data !== 'undefined'?data.phone:'')%>" placeholder="Số điện thoại">
                                </div>
                                <div class="form-group">
                                  <label for="username">Tài khoản</label>
                                  <input type="text" class="form-control" name="username" id="username" value="<%=(typeof data !== 'undefined'?data.username:'')%>" placeholder="Tài khoản">
                                </div>
                              </div>
                              <div class="col-md-4">
                                <div class="form-group">
                                  <label for="password">Mật khẩu</label>
                                  <input type="password" class="form-control" name="password" id="password" value="<%=(typeof data !== 'undefined'?data.password:'')%>" placeholder="Mật khẩu">
                                </div>
                                <div class="form-group">
                                  <label for="password_reply">Mật khẩu</label>
                                  <input type="password" class="form-control" name="password_reply" id="password_reply" value="<%=(typeof data !== 'undefined'?data.password_reply:'')%>" placeholder="Mật khẩu xác nhận">
                                </div>
                              </div>
                          </div>
                          <div class="text-right" >

                              <button type="submit" class="btn btn-primary btn-sm " >Đăng Ký Bán Hàng </button>
        
                          </div>
                        </form>
                        <hr/>
                        <div class="">
                           Bạn có thể đăng nhập bằng 
                            <a href="/auth/facebook" class="btn btn-primary btn-sm"> <i class="fa fa-facebook"> Facebook</i></a>
                            <a href="/auth/google" class="btn btn-danger btn-sm"> <i class="fa fa-google"> Google</i></a>
                            <a href="/auth/zalo"  class="btn btn-success btn-sm" style="font-size: 14px;font-family:'FontAwesome' ;"><img src="../../img/zalo.png" style="width:20px;">  Zalo</a>
                        </div>
                    </div>
                  </div>
            </div>
        </div>
    </div>
</section>